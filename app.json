{
  "expo": {
    "name": "ovidio-app",
    "slug": "ovidio-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/ovidio_logo.png",
    "scheme": "ovidioapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#181719",
        "foregroundImage": "./assets/logos/ovidio_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logos/ovidio_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logos/ovidio_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#181719"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}