{
  "expo": {
    "name": "ovidio-app",
    "slug": "ovidio-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/ovidio_logo.png",
    "scheme": "ovidioapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abulbasar.ovidioapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#181719",
        "foregroundImage": "./assets/logos/ovidio_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.abulbasar.ovidioapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logos/ovidio_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logos/ovidio_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#181719"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a2078694-185b-446a-9672-7dbd4598cd33"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b14cc64e-0e74-4506-a6ac-aaa4cbbb8992"
    }
  }
}
